{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "commonJs",
    "target": "es5",
    "lib": [
      "es2019","DOM","DOM.Iterable", "ScriptHost"
    ],
    "allowJs": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "sourceMap": true
  }
}
